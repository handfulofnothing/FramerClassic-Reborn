{
  "name": "framerjs",
  "version": "3.0.0",
  "description": "Modern Prototyping Framework",
  "type": "module",
  "main": "./dist/framer.js",
  "module": "./dist/framer.js",
  "exports": {
    ".": {
      "import": "./dist/framer.js",
      "default": "./dist/framer.js"
    }
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koenbok/Framer.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "keywords": [
    "prototyping"
  ],
  "author": "Koen Bok",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koenbok/Framer/issues"
  },
  "homepage": "http://framer.com",
  "dependencies": {
    "hsluv": "^1.0.1",
    "lodash-es": "^4.17.21",
    "webfontloader": "^1.6.28"
  }
}
